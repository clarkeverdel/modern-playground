{
  "extends": [
    "eslint:recommended",
    "next/core-web-vitals",
    "plugin:import/recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint",
    "jsx-a11y"
  ],
  "rules": {
    "@typescript-eslint/rule-name": "error",
    "jsx-a11y/rule-name": 2
  },
  "parserOptions": {
    "project": "./tsconfig.lint.json"
  }
}
